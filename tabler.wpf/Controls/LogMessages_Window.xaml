<Window x:Class="tabler.wpf.Controls.LogMessages_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tabler.wpf.Controls"
        xmlns:controls="clr-namespace:tabler.wpf.Controls"
        mc:Ignorable="d"
        Title="LogMessages_Window" d:DesignHeight="300" d:DesignWidth="300">
    <Grid Height="Auto" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="1" Name="grdMainContent">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <controls:Helper_LogMessages></controls:Helper_LogMessages>
    </Grid>
</Window>
